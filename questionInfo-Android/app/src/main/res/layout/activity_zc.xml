<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/a"
    tools:context=".ZcActivity">

    <TextView
        android:id="@+id/tvUserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvRealName"
        android:layout_marginBottom="172dp"
        android:padding="10dp"
        android:text="账号:"
        android:textColor="#000000"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tvRealName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tvNo"
        android:layout_marginTop="86dp"
        android:padding="10dp"
        android:text="姓名:"
        android:textColor="#000000"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tvNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvPWd"
        android:layout_alignTop="@+id/etNo"
        android:padding="10dp"
        android:text="学号:"
        android:textColor="#000000"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tvPWd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="39dp"
        android:layout_marginTop="149dp"
        android:padding="10dp"
        android:text="密码:"
        android:textColor="#000000"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tvPWd1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tvUserName"
        android:layout_marginTop="213dp"
        android:padding="10dp"
        android:text="确认密码:"
        android:textColor="#000000"
        android:textSize="20sp" />


    <TextView
        android:id="@+id/tvZh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvUserName"
        android:layout_marginBottom="240dp"
        android:padding="10dp"
        android:text="组号:"
        android:textColor="#000000"
        android:textSize="20sp" />


    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="42dp"
        android:layout_marginStart="77dp"
        android:text="注册"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button"
        android:layout_marginEnd="69dp"
        android:text="返回登录"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/etNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="21dp"
        android:layout_toEndOf="@+id/tvPWd1"
        android:ems="10"
        android:hint="学号 "
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/etRealName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvRealName"
        android:layout_toEndOf="@+id/tvPWd1"
        android:ems="10"
        android:hint="姓名"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/etPwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvPWd"
        android:layout_toEndOf="@+id/tvPWd1"
        android:ems="10"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/etPwd1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvPWd1"
        android:layout_toEndOf="@+id/tvPWd1"
        android:ems="10"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/etZh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvZh"
        android:layout_toEndOf="@+id/tvPWd1"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="组号" />

    <EditText
        android:id="@+id/etUserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvUserName"
        android:layout_toEndOf="@+id/tvPWd1"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="用户名" />

</RelativeLayout>